<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Adicionar</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Adicionar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSabores</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.GetSabores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ativar</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Ativar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Editar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Entrar</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Entrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Manutencao</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Manutencao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Remover</servlet-name>
        <servlet-class>br.com.pizzariadomenico.Process.Remover</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Adicionar</servlet-name>
        <url-pattern>/Adicionar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSabores</servlet-name>
        <url-pattern>/GetSabores</url-pattern>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ativar</servlet-name>
        <url-pattern>/Ativar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar</servlet-name>
        <url-pattern>/Editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Entrar</servlet-name>
        <url-pattern>/Entrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Manutencao</servlet-name>
        <url-pattern>/Manutencao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Remover</servlet-name>
        <url-pattern>/Remover</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    
</web-app>
